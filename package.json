{
  "name": "un-detector",
  "version": "1.0.1",
  "packageManager": "pnpm@8.10.5",
  "description": "A simple and zero-dependency browser, os and device detector.",
  "type": "module",
  "keywords": [
    "browser detector",
    "browser type",
    "node type",
    "os",
    "os detector",
    "detect",
    "useragent"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/un-detector.git"
  },
  "author": "hemengke <https://github.com/hemengke1997>",
  "files": [
    "CHANGELOG.md",
    "README.md",
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "simple-git-hooks && tsup --watch",
    "build": "tsup",
    "build:pages": "pnpm run build && cd playground/spa && pnpm run build",
    "test": "vitest",
    "test:unit": "vitest --run",
    "lint": "eslint . --fix",
    "up": "taze -I -w",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "bump": "bumpp package.json -c -p -t --all -x \"pnpm run changelog\""
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@minko-fe/commitlint-config": "^2.0.7",
    "@minko-fe/eslint-config": "^2.1.2",
    "@minko-fe/tsconfig": "^2.0.7",
    "@types/node": "^20.11.16",
    "bumpp": "^9.3.0",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.56.0",
    "happy-dom": "^12.10.3",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.1.2",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.13.3",
    "tsup": "^6.7.0",
    "typescript": "^5.3.3",
    "vite": "^4.5.2",
    "vitest": "^0.34.6",
    "vitest-e2e": "^0.0.10"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm exec commitlint -e",
    "pre-commit": "pnpm run lint"
  }
}
